<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
	PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
	"http://java.sun.com/j2ee/dtds/web-app_2_2.dtd"
>


<web-app>
  <description>webadmin</description>

  <servlet>
    <servlet-name>rest</servlet-name>
    <servlet-class>de.willuhn.jameica.webadmin.servlets.RestServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>rest</servlet-name>
    <url-pattern>/rest/*</url-pattern>
  </servlet-mapping>

  <taglib>
    <taglib-uri>
      http://www.willuhn.de/projects/jameica/taglib
    </taglib-uri>
    <taglib-location>/WEB-INF/jameica.taglib.tld</taglib-location>
  </taglib>
  
</web-app>
