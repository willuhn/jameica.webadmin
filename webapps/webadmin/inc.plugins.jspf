<jsp:useBean id="plugin" class="de.willuhn.jameica.webadmin.beans.Plugin"/>

<h2>Installierte Plugins</h2>
<table class="data">
  <tr>
    <th>Name</th>
    <th>Version</th>
    <th>Beschreibung</th>
  </tr>
  <c:forEach var="p" items="${plugin.plugins}">
     <tr>
       <td>${p.name}</td>
       <td>${p.version}</td>
       <td>${p.description}</td>
     </tr>
  </c:forEach>
</table>
